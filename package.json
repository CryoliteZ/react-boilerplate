{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "React x Webpack Boilerplate with Babel, SASS and Ant Design",
  "author": "Alvin Hou",
  "license": "MIT",
  "homepage": "https://github.com/CryoliteZ/react-boilerplate",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --progress --mode development --config webpack.config.dev.js",
